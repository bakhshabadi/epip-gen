{
  "name": "epip-generate",
  "version": "1.2.71",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "main": "./src/main.js",
  "bin": {
    "epip-generate": "./src/main.js"
  },
  "preferGlobal": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "up-version": "npm version patch --force",
    "publish-app": "npm publish --registry http://5.161.193.73:4873/",
    "build:app": "npm run up-version && rmdir /q /s dist && tsc -p ./tsconfig.json && copy package.json dist\\package.json",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen:help": "nest build && node dist\\main.js gen --help",
    "start": "rimraf dist & npm run build & node dist\\main.js gen -t vue -o ./ -h http://localhost:8585/api/v1/swagger.json",
    "swagger": "ts-node src/main.ts  vue -o ./dist -p to  -h http://localhost:8585/v1/api/swagger.json -in @/core/interceptor -e VITE_APP_URL -s 2",
    "start:check": "ts-node src/main.ts version",
    "start:dev": "ts-node src/main.ts vue -o ./dist -p to -t auth,patient,physo,vidu,paypal -h https://api.test.hemscap.com/v1/api/swagger.json -in @/core/interceptor -e VITE_APP_URL -s 2",
    "start:debug": "nest start --debug --watch",
    "start:jahan": "ts-node src/main.ts vue -o ./dist -t auth,patient,paypal,vidu,physo -h http://5.161.193.73:8585/v1/api/swagger.json -in @/core/interceptor",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "async": "^3.2.4",
    "await-to-js": "^3.0.0",
    "axios": "^1.3.2",
    "kebab-case": "^1.0.2",
    "lodash": "^4.17.21",
    "nest-commander": "^2.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "snake-case": "^3.0.4",
    "text-cleaner": "^1.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.2.5",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
